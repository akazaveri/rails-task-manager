<div class="container mt-5">
  <h1>New task</h1>
  <div class="mt-3">
    <%= form_with model: @task do |f| %>
      <%= f.label :title %><br>
      <%= f.text_field :title %><br>
      <br>
      <%= f.label :details %><br>
      <%= f.text_field :details %><br>
      <br>
      <%= f.label :completed %><br>
      <%= f.check_box :completed %><br>
      <br>
      <%= f.submit %>
    <% end %>
  </div>
</div>
